mixin footerBoxes(titulo,enlaces)
  .box
    h2.box__title= titulo
    ul.box__links-container 
      each enlace in enlaces 
        li.box__links-item: a.box__links-link= enlace

mixin menuBox(menu)
  a.menu-box(href="#")
    img.menu-box__img(src= menu[1], alt="")
    span.menu-box__title= menu[0]

mixin addressBox(address)
  .addressBox 
    h2.addressBox__position= address[0]
    p.addressBox__horario= address[1] 
    p.addressBox__horario2= address[2]
